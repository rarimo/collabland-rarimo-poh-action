{
  "name": "collabland-rarimo-poh-action",
  "description": "Implementation of the Collab.Land action /verify command for the Discord server which allows members to verify their humanity using the Rarimo Proof of Humanity case.",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Zero Block Global Foundation",
  "keywords": [
    "rarimo",
    "collabland",
    "proof-of-humanity"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rarimo/collabland-rarimo-poh-action.git"
  },
  "gitHooks": {
    "pre-commit": "yarn lint",
    "pre-push": "yarn rsc"
  },
  "scripts": {
    "dev": "yarn migrate-up && yarn dev-server",
    "dev-server": "next dev --port 8000",
    "build": "next build",
    "start": "next start --port 8000",
    "lint": "yarn lint:scripts && yarn lint:styles && yarn tsc --noEmit",
    "lint:scripts": "next lint",
    "lint:styles": "stylelint \"src/**/*.{css,sass,scss}\" --max-warnings=0",
    "rsc": "node scripts/release-sanity-check.mjs",
    "migrate-up": "knex migrate:up ",
    "migrate-down": "knex migrate:down"
  },
  "dependencies": {
    "@collabland/discord": "^0.27.0",
    "@collabland/models": "^0.28.0",
    "@distributedlab/fetcher": "^1.0.0-rc.8",
    "@distributedlab/w3p": "^1.0.0-rc.8",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@formatjs/intl-localematcher": "^0.4.0",
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.3",
    "@walletconnect/modal": "^2.6.2",
    "bigint-buffer": "^1.1.5",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "knex": "^3.0.1",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.1",
    "mitt": "^3.0.1",
    "next": "^13.5.5",
    "next-international": "^1.1.4",
    "pg": "^8.11.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.48.2",
    "react-use": "^17.4.0",
    "tweetnacl": "^1.0.3",
    "winston": "^3.11.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@comparaonline/stylelint-config-scss-modules": "^1.1.0",
    "@types/lodash": "^4",
    "@types/node": "20.4.5",
    "@types/pg": "^8",
    "@types/prettier": "^3",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "next-circular-dependency": "^1.0.1",
    "prettier": "^3.0.0",
    "sass": "^1.64.2",
    "stylelint": "^15.10.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-declaration-strict-value": "^1.9.2",
    "stylelint-prettier": "^4.0.2",
    "stylelint-scss": "^5.0.1",
    "typescript": "^5.2.2",
    "yorkie": "^2.0.0"
  },
  "packageManager": "yarn@4.0.1",
  "resolutions": {
    "@collabland/discord/discord-api-types": "0.37.50",
    "discord.js/discord-api-types": "0.37.50"
  }
}
