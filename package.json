    "@mui/material": "^5.14.3",
    "loglevel": "^1.8.1",
    "next": "^13.5.5",
    "next-international": "^0.9.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "winston": "^3.11.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@comparaonline/stylelint-config-scss-modules": "^1.1.0",
