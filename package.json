    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.0.0",
    "sass": "^1.64.2",
    "stylelint": "^15.10.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-declaration-strict-value": "^1.9.2",
    "stylelint-prettier": "^4.0.2",
    "stylelint-scss": "^5.0.1",
    "typescript": "^5.2.2",
    "yorkie": "^2.0.0"
